{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint \"**/*.{js,ts,jsx,tsx}\" --fix",
    "start": "react-native start",
    "start:staging": "APP_ENV=staging react-native start",
