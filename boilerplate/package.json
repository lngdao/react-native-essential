    "dayjs": "^1.11.10",
    "i18next": "^23.11.2",
    "immer": "^10.0.4",
    "import": "^0.0.6",
    "ky": "^1.2.3",
    "react": "18.2.0",
    "react-fast-compare": "^3.2.2",
